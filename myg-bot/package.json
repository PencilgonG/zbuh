{
  "name": "myg-bot",
  "version": "1.0.0",
  "description": "Bot Discord MYG Inhouse â€“ Discord.js v14 + Prisma + Neon",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "node dist/index.js",
    "register:guild": "tsx -r dotenv/config scripts/register-commands.ts --scope guild",
    "register:global": "tsx -r dotenv/config scripts/register-commands.ts --scope global",
    "cleanup": "tsx scripts/cleanup-commands.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "lint": "tsc --noEmit",
    "prepare": "prisma generate"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@discordjs/builders": "1.9.0",
    "@prisma/client": "6.19.0",
    "discord-api-types": "0.37.83",
    "discord.js": "14.16.3",
    "dotenv": "17.2.1",
    "pino": "9.9.0",
    "zod": "3.23.8",

    "puppeteer": "^23.7.0"
  },
  "devDependencies": {
    "@types/node": "24.9.1",
    "@types/puppeteer": "^7.0.4",
    "pino-pretty": "13.1.2",
    "prisma": "6.19.0",
    "tsx": "4.19.2",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT"
}
